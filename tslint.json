{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-string-literal": true
  },
  "jsRules": {
    "no-console": false,
    "ordered-imports": false,
    "no-string-literal": true
  }
}